{
  "name": "sql-server",
  "version": "1.0.0",
  "description": "First learn sql server",
  "main": "index.js",
  "scripts": {
    "server": "nodemon dist/index.js",
    "dev": "nodemon --exec ts-node index.ts",
    "build": "tsc -p .",
    "pm2start": "pm2 start dist/index.js -o output.json -e errors.json",
    "pm2restart": "pm2 restart dist/index.js --update-env",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/db/dbConfig.ts",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:create": "npm run typeorm migration:create -- -n"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.17",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemon": "^1.19.1",
    "@types/pg": "^8.6.4",
    "@types/uuid": "^8.3.4",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.2",
    "googleapis": "^101.0.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.3",
    "nodemon": "^2.0.15",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "typeorm": "^0.2.45",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  }
}
